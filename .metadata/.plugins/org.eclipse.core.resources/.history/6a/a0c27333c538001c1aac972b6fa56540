	List<RoomDTO> stage4(List<RoomDTO> memberList) {
		Scanner scan = new Scanner(System.in);
		RoomDTO m = null;
		m = memberList.get(num);

		boolean runF = true;

		while (runF) {

			boolean run = true;

			while (run) {
	
					System.out.println("");
