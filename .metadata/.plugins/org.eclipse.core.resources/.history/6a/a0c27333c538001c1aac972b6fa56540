	List<RoomDTO> bonusStage(List<RoomDTO> memberList) {
		Scanner scan = new Scanner(System.in);
		RoomDTO m = null;
		m = memberList.get(num);

		boolean runF = true;
		boolean run = true;
		while (runF) {
			System.out.println("\nìœ¼ìœ½..ëˆˆì„ ë–  ë³´ë‹ˆ ê±°ëŒ€í•œ ì‚¬ëŒì´ ë‹¤ê°€ì˜¤ê³  ìˆìŠµë‹ˆë‹¤." 
					+ "\nê·¸ ì‚¬ëŒì€ í—¬ìŠ¤ì— ì¤‘ë…ëœ ì§€ì„ì´ì˜ ê¹ë¶€ í˜•ì² ì´ì—ˆìŠµë‹ˆë‹¤.ğŸ˜±");
			System.out.println("\n\n[í˜•ì²  : ê°íˆ ë‚´ ê¹ë¶€ì—ê²Œ ì¤€ ì¼€ì´í¬ë¥¼ ë¨¹ë‹¤ë‹ˆ!!ğŸ˜¡]" 
					+ " \n ë¶„ë…¸í•œ í˜•ì² ì´ê°€ ë‹¹ì‹ ì—ê²Œ 600ë§Œì›ì§œë¦¬ í”¼í‹° ê°€ì…ë™ì˜ì„œë¥¼ ë“¤ê³  ê°•ì œë¡œ ê°€ì… ì‹œí‚¤ë ¤ ë‹¤ê°€ì˜µë‹ˆë‹¤.");
			System.out.println("ë†€ë€ ë‹¹ì‹ ì€ ì£¼ìœ„ì— ë†“ì—¬ì§„ ë¬¼ê±´ë“¤ì„ ì£¼ì›Œ í˜•ì² ì´ì—ê²Œ ë˜ì§€ë ¤ í•©ë‹ˆë‹¤.");
			System.out.println("ì—”í„°ë¥¼ ëˆŒëŸ¬ ë¬¼ê±´ì„ ë˜ì§€ì„¸ìš”!!");

			String[] weapon = { "ì€ì§€ì˜ ë§ˆì¹´ë¡±ğŸ‚","ê´‘ìš±ì´ì˜ ë§¥ ë…¸íŠ¸ë¶ğŸ’»", "ìœ¤ì„±ì˜ ì˜ì–‘ì œí†µğŸ’Š", "ë‹¹ì‹  ì£¼ë¨¸ë‹ˆì˜ ë™ì „ğŸ’°" };

			int hp = 100;
			System.out.println("í˜•ì² ì˜ hp : " + hp);

			while (run) {
				if (hp >= 0) {
					int w = (int) (Math.random() * 4);
					scan.nextLine();
					
					System.out.println("ë‹¹ì‹ ì€ " + weapon[w] + "ì„ ë˜ì¡ŒìŠµë‹ˆë‹¤!");

					if (weapon[w].equals(weapon[0])) {
						System.out.println("\n[í˜•ì²  : ìœ½ ! ì´ê±´ ë‹¹ë¶„...!!!ë„ˆë¬´ ë‹¬ë‹¬í•´!!] \n <í˜•ì² ì—ê²Œ 40ì˜ ë°ë¯¸ì§€ë¥¼ ì…í˜”ìŠµë‹ˆë‹¤!>");
						hp -= 40;
						System.out.println("í˜•ì² ì˜ hp : " + hp);

					} else if (weapon[w].equals(weapon[1])) {
						System.out.println("\n í˜•ì² ì€ ë…¸íŠ¸ë¶ì„ ê°€ë³ê²Œ í•œ ì†ìœ¼ë¡œ ë°›ì•˜ìŠµë‹ˆë‹¤.\ní˜•ì²  ì†ì˜ êµ³ì€ ì‚´ì„ ì¢€ ë–¼ì–´ëƒˆìŠµë‹ˆë‹¤."
								+ " \n[í˜•ì²  : ë‚´ êµ³ì€ ì‚´ì´!!!]\n <í˜•ì² ì—ê²Œ 2ì˜ ë°ë¯¸ì§€ë¥¼ ì…í˜”ìŠµë‹ˆë‹¤!>");
						hp -= 2;
						System.out.println("í˜•ì² ì˜ hp : " + hp);

					} else if (weapon[w].equals(weapon[2])) {
						System.out.println("\n í˜•ì² ì´ ì˜ì–‘ì œë¥¼ ê°€ë³ê²Œ ë°›ì•„ ì˜ì–‘ì œë¥¼ í•œ ì•Œ ê¹Œë¨¹ê³  ë‹¹ì‹ ì—ê²Œ ë‹¤ê°€ì˜µë‹ˆë‹¤.");
						hp += 5;
						System.out.println("í˜•ì² ì˜ hp : " + hp);

					} else {
						int coin = (int) (Math.random() * 30) + 1;
						System.out.println("\në•¡ê·¸ë‘! ì£¼ë¨¸ë‹ˆì—ì„œ í—ˆê²ì§€ê² êº¼ë‚¸ ë™ì „ì€" + coin + "ì› ì…ë‹ˆë‹¤.");
						System.out.println("[í˜•ì²  : ì•„ì–!]\n" + "<í˜•ì² ì—ê²Œ" + coin + "ì˜ ë°ë¯¸ì§€ë¥¼ ì…í˜”ìŠµë‹ˆë‹¤!>");
						hp -= coin;
						m.setMoney(m.getMoney()-coin);
						System.out.println("ë‹¹ì‹  ì§€ê°‘ì˜ ì”ê³  : "+m.getMoney());
						System.out.println("í˜•ì² ì˜ hp : " + hp);
					}
				}
			}
			runF = false;
			System.out.println("\nğŸŠğŸŠğŸŠğŸŠâ˜†â˜…â˜†â˜…â˜†â˜…â˜†â˜…â˜†â˜…â˜†â˜…â˜†â˜…â˜†â˜…â˜†â˜…â˜†â˜…â˜†â˜…â˜†â˜…â˜†â˜…â˜†â˜…â˜†â˜…â˜†â˜…â˜†â˜…â˜†â˜…â˜†â˜…â˜†â˜…â˜†â˜…â˜†â˜…â˜†â˜…â˜†â˜…â˜†â˜…â˜†\n");
			System.out.println("í˜•ì² ì´ê°€ ì“°ëŸ¬ì¡ŒìŠµë‹ˆë‹¤!!!");
			System.out.println("ë³´ë„ˆìŠ¤ ìŠ¤í…Œì´ì§€ í´ë¦¬ì–´! ë‹¹ì‹ ì€ í˜•ì² ì´ë¥¼ ë¬¼ë¦¬ì¹œ ê¹€ì— 6ë§Œì›ì„ ë¹¼ì•—ê³  ì¼€ì´í¬ ì•ˆì˜ ì—´ì‡ ë¥¼ ë°œê²¬í–ˆìŠµë‹ˆë‹¤!");
			System.out.println("\nğŸŠğŸŠğŸŠğŸŠâ˜†â˜…â˜†â˜…â˜†â˜…â˜†â˜…â˜†â˜…â˜†â˜…â˜†â˜…â˜†â˜…â˜†â˜…â˜†â˜…â˜†â˜…â˜†â˜…â˜†â˜…â˜†â˜…â˜†â˜…â˜†â˜…â˜†â˜…â˜†â˜…â˜†â˜…â˜†â˜…â˜†â˜…â˜†â˜…â˜†â˜…â˜†â˜…â˜†â˜…â˜†\n");
			m.setMoney(m.getMoney() + 60000);
			System.out.println("ë‹¹ì‹ ì˜ ì •ë³´ : " + m);
		}

		return memberList;
	}


